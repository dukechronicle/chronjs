.survey(id=survey.id)
  .container.clearfix
      .vertical-container
          h2.vertical Survey

      .rounded
          .content-container
              h1 #{survey.title}
              ul.choices
              - each votes, choice in survey.answers
                  il.choice(id=choice, votes=votes)
                      a.hover(href="#")
                          h2 #{choice}
