        title qDuke.com
    body
        #wrap
            .rowHead.clearfix
                a.box.boxLogo(href="http://qduke.com")
                    h1
                        em q
                        span Duke
                a.box(href="http://www.weather.com/weather/right-now/Durham+NC+27708") Weather
                a.box(href="https://webmail.duke.edu/") Webmail
                a.box(href="http://gmail.com/") Gmail
                a.box(href="http://facebook.com/") Facebook
            // NEWS
            .rowNews.clearfix
                a.box.boxChron(href="http://dukechronicle.com") The Chronicle
                .boxStories
                    a.box.boxEmpty
                    a.box.boxEmpty
                    a.box.boxEmpty
                    a.box.boxEmpty
            // SEARCH
            .rowSearch.clearfix
                .box.boxSearch
                    input(name="query", type="text", style="background:transparent", onkeydown="searchOnEnter(event)")
                a.box.boxButton(href='javascript:search("Google");') Google
                a.box.boxButton(href='javascript:search("WolframAlpha");') WolframAlpha
                a.box.boxButton(href='javascript:search("Duke");') Duke
                - var linkRows = Math.ceil(links.length/5)
            //- // MENU
            .menu.clearfix
                a.box.selected(href="javascript:changeTab(1)") Quick Links
                span.spacer.colsmargin
                a.box(href="javascript:changeTab(2)") Bus Tracking
                span.spacer.colsmargin
                a.box(href="javascript:changeTab(3)") Campus Map
                //- span.spacer.colsmargin
                //- a.box(href="javascript:changeTab(4)") Sports
                span.spacer.colsmargin
                span.spacer.cols2
            // LINKS
            #tabFrame
                #contentLinks.tabContent.tabShown
                    .rowLinks.clearfix
                        -for (var i = 0; i < 5; i++)
                            a.box.boxLink.boxLinkTall(href=links[i].url) #{links[i].title}
                    - for (var row = 1; row < linkRows; row++)
                        .rowLinks.clearfix
                            -for (var i = row*5; i < row*5+5; i++)
                                a.box.boxLink(href=links[i].url) #{links[i].title}
                #contentFrame.box.tabContent(data-content='<object type="application/x-shockwave-flash" width="100%" height="100%" data="//static.transloc.com/tvspublic/_common/swf/TvsClient-default.swf?v=6619a32d2bacada0b3703d86a4b8efaa"><param name="src" value="//static.transloc.com/tvspublic/_common/swf/TvsClient-default.swf?v=6619a32d2bacada0b3703d86a4b8efaa"><param name="bgColor" value="#ffffff"><param name="wmode" value="transparent"><param name="allowScriptAccess" value="always"><param name="autoplay" value="false"><param name="flashvars" value="box=35.994114%2C-78.955356%2C36.022726%2C-78.914718&amp;feeds_domain=feeds.transloc.com&amp;tile_server_parent=transloc.com&amp;sms=1&amp;predictions=True&amp;agencies=176&amp;static_media_domain=static.transloc.com&amp;announcements=True&amp;sms_keyword=DUKEU&amp;classic=1"><div><p><strong>Flash Required</strong></p><p>Flash is required to view this media. <a href="http://www.adobe.com/go/getflashplayer">Download Here</a>.</p></div></object>')
                    a#BusTracking.newWindow(href="http://www.duke.transloc.com/")
                #contentFrame.box.tabContent(data-content='<iframe src="http://maps.duke.edu/map/index.php?id=21"></iframe>')
                    a#CampusMap.newWindow(href="http://map.duke.edu/")
            // ADS
            .menu.clearfix
                .box.selected Sponsored
                span.spacer.colsmargin
                span.spacer.cols4
            - for (var row = 0; row < 2; row++)
                .rowAds.clearfix
                    -for (var i = row*5 + 1; i <= row*5+5; i++)
                        |<script type='text/javascript'><!--// <![CDATA[
                        |                     OA_show('zone_946_#{i}');
                        |                     // ]]> --></script>

