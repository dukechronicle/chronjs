extends layout

block content
    #commencement-2012
        .lookbacks.clearfix.align-group
            - each story, year in lookbacks
                - if (year == "senior")
                    .span-3.last.align-element(data-aligntarget=".rounded")!=partial('../container/block/teaser', { story: story, title: year })
                - else
                    .span-3.align-element(data-aligntarget=".rounded")!=partial('../container/block/teaser', { story: story, title: year })

        .middle.clearfix
            .rounded.left
                .image-container
