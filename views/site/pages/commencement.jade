            .calendar.left(style="margin-left:30px;")
                .vertical-container
                    h2.vertical Calendar

                .rounded(style="width:542px;height:306px;")
                    .content-container
                        - each day in schedule
                            a.hover.highlight-right(href="/article/commencement-2012-event-calendar")
                                h3.date= day.date
                                ul.events
                                    - each event in day.events
                                        li: p= event

        .bottom.clearfix
            - each story in articles
                .span-4!=partial('../container/block/wide', { story: story })
            .span-4.last!=partial('../container/ad/medium-rect', {title: "Advertisement", index: 1})