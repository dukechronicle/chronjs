#frontpage
    #top.clearfix
        .content.span-8
            if (model["Top Headline"])
                #top-headline!=partial('../container/story-list', {stories: model["Top Headline"]})
            if (model.Breaking)
                #breaking!=partial('../container/breaking', {stories: model.Breaking})
            if (model.Slideshow)
                #slideshow!=partial('slideshow', {stories: model.Slideshow})
            if (model["Left Headlines"] && model["Right Headlines"])
                .align-group#slideshow!=partial('../container/top-news', {title: "Top Stories", left: model["Left Headlines"], right: model["Right Headlines"]})

            .ads-row.clearfix
                if (model.printEdition)
                    .lighten.span-2#print-edition!=partial('../container/image', model.printEdition)
                .lighten.span-6.last!=partial('../container/ad/full-banner', {title: "AD"})

        .sidebar.span-4.last
            //
                if (model.twitter)
                    .twitter!=partial('../container/twitter', model.twitter)
            if (model.popular)
                .most-popular!=partial('../container/articles-with-subhead', {title: "What's Hot", stories: model.popular})
            if (model.Opinion)
                .opinion!=partial('../container/articles-with-subhead', {title: "Opinion", stories: model.Opinion})
            .lighten!=partial('../container/ad/medium-rect', {title: "Advertisement"})
