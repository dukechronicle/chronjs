extends layout

block content
    #recess
        #top.clearfix.align-group
            .span-8.align-element(data-alignprimary="true")
                #slideshow!=partial('../container/slideshow', {stories: model.Featured})

            .span-4.last.align-element(data-aligntarget="#editors-notes .rounded")
                !=partial('../container/ad/medium-rect', {title: "Advertisement"})
                #editors-notes.title-medium!=partial('../container/story-list/featured', {title: "Editor's Notes", stories: model.Stories})

        #middle.clearfix
            if (model.Music)
                 #reviews!=partial('../container/row/feature', {title: 'Music', stories: model.Music})
            if (model.Film)
                #interviews!=partial('../container/row/feature', {title: 'Film', stories: model.Film})
            if (model.Art)
                #sandbox!=partial('../container/row/feature', {title: 'Art', stories: model.Art})
            .span-12.last!=partial('../container/row/multimedia', {title: "Multimedia", multimedia: model.multimedia.Recess})
