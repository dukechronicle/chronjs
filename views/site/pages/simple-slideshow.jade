extends layout

block content
  #top.clearfix.align-group
    .span-8.align-element(data-alignprimary="true")
      #slideshow!=partial('../container/slideshow', {stories: slideshow})
    .span-4.last.align-element(data-aligntarget="#article-list .rounded")
      #article-list.title-small!=partial('../container/story-list', {title: list_title, stories: list})
  #bottom.clearfix
    - each article, i in articles
      .span-4(class=(i % 3 == 2 ? "last" : ""))!=partial('../container/block/square', {story: article})