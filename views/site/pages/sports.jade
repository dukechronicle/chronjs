#sports
    !=partial('../layout/subnav', {subsections: subsections})

    #top
        if (model.Slideshow)
            !=partial('../container/slideshow/slideshow-right', {stories: model.Slideshow})
    #bottom.clearfix
        .span-4
            if (model.adFullRectangle)
                .lighten!=partial('../container/ad/medium-rect', {title: "Advertisement"})
            if (model.Blog)
                .blog!=partial('../container/story-list-image', {locals: {title: "The Blue Zone", image: "http://d2sug25c5hnh7r.cloudfront.net/images/blogs/sportsblog-banner.jpg", imageHref:'http://sports.chronicleblogs.com', stories:model.Blog}})
            //

        .span-8.last.clearfix
            - if (model.Stories)
                #middle.clearfix
                    .span-4
                        if (model.Stories[0])
                            !=partial('../container/row-2', {story: model.Stories[0], tax: true})
                        if (model.Stories[2])
                            !=partial('../container/row-2', {story: model.Stories[2], tax: true})
                        if (model.Stories[4])
                            !=partial('../container/row-2', {story: model.Stories[4], tax: true})
                        if (model.Stories[6])
                            !=partial('../container/row-2', {story: model.Stories[6], tax: true})
                    .span-4.last
                        if (model.Stories[1])
                            !=partial('../container/row-2', {story: model.Stories[1], tax: true})
                        if (model.Stories[3])
                            !=partial('../container/row-2', {story: model.Stories[3], tax: true})
                        if (model.Stories[5])
                            !=partial('../container/row-2', {story: model.Stories[5], tax: true})
                        if (model.Stories[7])
                            !=partial('../container/row-2', {story: model.Stories[7], tax: true})
            !=partial('../container/multimedia', {title: "Multimedia", multimedia: model.multimedia.Sports})
