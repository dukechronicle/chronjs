.nav.clearfix
    for parent in parentPaths
        h2.selected
            a(href='/section' + parent.path) #{parent.name} |
    for subsection in subsections
        for subsectionName,subsectionPath in subsection
            h2
                a(href='/section/' + subsectionPath) #{subsectionName}
#section
    .articles
        for doc in docs
            a.result.rounded-hover(href=doc.url)
                h3 #{doc.title}
                .date #{doc.date}
                .teaser #{doc.teaser}
    .sidebar
        - if (typeof popular != 'undefined' && popular.length > 0)
            .most-popular!=partial('container/most-popular', {title: "Popular", stories: popular})
        // facebook recommend
        .container.clearfix.facebook-recommendations(style="margin-bottom: 20px")
            .vertical-container
                h2.vertical Facebook Recommended
            .rounded(style="width: 300px; height: 300px; background: white")
                .fb-recommendations(data-site="dukechronicle.com", data-border-color="white", data-width="300", data-height="300", data-font="lucida grande", data-header="true", max-age="7")
        script
            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) {return;}
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));

