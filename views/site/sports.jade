!=partial('site/subnav', {subsections: subsections})

#sports
    .top.clearfix
        if (model.Slideshow)
            .slideshow-wrapper.right-select!=partial('slideshow/slideshow-right', {stories: model.Slideshow})
    .middle.clearfix
        .left
            if (model.adFullRectangle)
                .image-container.medium-rectangle.lighten!=partial('container/image', model.adFullRectangle)
            //

        .right
            if (model.Stories)
                .column.first
                    if (model.Stories[0])
                        .first!=partial('container/row-2', {story: model.Stories[0]})
                    if (model.Stories[2])
                        .last!=partial('container/row-2', {story: model.Stories[2]})
                    //
                        if (model.Stories[4])
                            .last!=partial('container/row-2', {story: model.Stories[4]})
                .column
                    if (model.Stories[1])
                        .first!=partial('container/row-2', {story: model.Stories[1]})
                    if (model.Stories[3])
                        .last!=partial('container/row-2', {story: model.Stories[3]})
                    //
                        if (model.Stories[5])
                            .last!=partial('container/row-2', {story: model.Stories[5]})

    .middle2.clearfix
        if (model.Blog)
            .blog!=partial('container/story-list-image', {locals: {title: "The Blue Zone", image: "/images/blogs/sportsblog-banner.jpg", imageHref:'http://sports.chronicleblogs.com', stories:model.Blog}})
        .multimedia!=partial('container/multimedia-sports', {title: "multimedia"})
    .bottom
        .row.clearfix
            if (model.Football)
                .football.first!=partial('container/sports-block.jade', {title: "Football", stories: model.Football})
            if (model.MSoccer)
                .m-soccer!=partial('container/sports-block.jade', {title: "Men's Soccer", stories: model.MSoccer})
            if (model.WSoccer)
                .w-soccer.last!=partial('container/sports-block.jade', {title: "Women's Soccer, stories: model.WSoccer"})
        //
            .row.clearfix.last
                if (model.WBasketball)
                    .w-basketball!=partial('container/sports-block.jade', {title: "Women's Basketball"})
                if (model.WSoccer)
                    .w-soccer!=partial('container/sports-block.jade', {title: "Women's Soccer"})
                if (model.WTennis)
                    .w-tennis.last!=partial('container/sports-block.jade', {title: "Women's Tennis"})
