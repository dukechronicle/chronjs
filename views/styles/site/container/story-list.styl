.story-list-subhead
    .title
        margin-bottom 0px

.list-story
    margin-bottom .7em

    h5.title
        font-size 1.1em
        line-height 100%

    .subhead
        font-family "Lucida Grande", "Lucida Sans Unicode", Helvetica, Arial, Verdana, sans-serif
        font-size 0.7em
        padding-top 0px
        text-transform uppercase
        margin-bottom 5px

        span
            padding-left 5px
            margin-left 5px
            border-left 1px solid #888

       .subhead span:first-child
            padding-left 0px
            margin-left 0px
            border-left 0px

.list-story:last-child
    margin-bottom 0em

.numbered-articles
    ol
        margin 0px
        li
            font-size 1em

