@import '../../mixins'
@import '../variables'


nav.subnav
    a h2
        float left
        letter-spacing 1px
        text-shadow 0px 1px 0px white
        font-size 0.9em
        text-transform uppercase
        color text-color
        display block
        padding-left 15px
        padding-right 11px
        margin-bottom 16px
        margin-top -16px

    a.selected:hover h2
        text-decoration line-through